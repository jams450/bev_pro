		<parameter name="ID_ODP" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[Select productos.nombre as Nombre, mp_odp.idinven as 'No inventario', inventario.lote as Lote, mp_odp.cantidad as Cantidad, umedida.nombre as UMedida, productos.clave  as Clave from mp_odp 
join inventario on inventario.id = mp_odp.idinven join productos on inventario.idproducto = productos.id
join umedida on umedida.id = productos.idmedida where mp_odp.idodp =  $P{ID_ODP}   and productos.idcategoria = 1]]>
		</queryString>
