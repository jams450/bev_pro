	<subDataset name="Dataset1" uuid="08b87704-f92b-4721-9f7e-e6992f3fc3d3">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="Trabajo"/>
		<property name="com.jaspersoft.studio.data.sql.tables" value=""/>
		<parameter name="ID_ODP" class="java.lang.String"/>
		<parameter name="factor" class="java.lang.Double"/>
		<parameter name="unidades" class="java.lang.Double"/>
		<queryString>
			<![CDATA[Select inventario.id, productos.nombre as Nombre, inventario.lote as Lote, mp_odp.cantidad as Cantidad, umedida.nombre as UMedida, 
productos.clave  as Clave , productos.pventa  as Costo, monedas.nombre as mo
from mp_odp 
join inventario on inventario.id = mp_odp.idinven 
join productos on inventario.idproducto = productos.id
join monedas on productos.idmoneda = monedas.id
join umedida on umedida.id = productos.idmedida where mp_odp.idodp =  $P{ID_ODP}]]>
		</queryString>
		<field name="id" class="java.lang.Integer">
			<property name="com.jaspersoft.studio.field.label" value="id"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="inventario"/>
		</field>
