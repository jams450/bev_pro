		</field>
		<field name="UMedida" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="UMedida"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="umedida"/>
		</field>
		<field name="Clave" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="Clave"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="productos"/>
		</field>
		<field name="Costo" class="java.lang.Double">
			<property name="com.jaspersoft.studio.field.label" value="Costo"/>
			<property name="com.jaspersoft.studio.field.tree.path" value="productos"/>
		</field>
		<field name="mo" class="java.lang.String">
			<property name="com.jaspersoft.studio.field.label" value="mo"/>
