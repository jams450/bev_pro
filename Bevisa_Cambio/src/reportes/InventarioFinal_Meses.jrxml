		<queryString>
			<![CDATA[select productos.nombre, sum(inventario.cantidadactual), umedida.nombre, productos.clave
from inventario
 join productos on productos.id = inventario.idproducto 
join umedida on umedida.id = productos.idmedida
where (productos.idcategoria = $P{c1}   or productos.idcategoria =  $P{c2} )  and month(fechaentrada) <= $P{mes}  and year(fechaentrada) <= $P{a} and inventario.cantidadactual >0
group by productos.nombre order by productos.nombre asc]]>
		</queryString>
