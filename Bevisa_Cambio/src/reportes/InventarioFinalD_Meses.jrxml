		<parameter name="a" class="java.lang.String"/>
		<parameter name="c1" class="java.lang.String"/>
		<parameter name="c2" class="java.lang.String"/>
		<parameter name="factor" class="java.lang.Double"/>
		<parameter name="mes" class="java.lang.String"/>
		<queryString>
			<![CDATA[select productos.nombre, sum(inventario.cantidadactual), productos.clave,umedida.nombre, productos.pventa,monedas.nombre
from inventario 
join productos on productos.id = inventario.idproducto 
join umedida on umedida.id = productos.idmedida 
join monedas on monedas.id=productos.idmoneda
where (productos.idcategoria = $P{c1}   or productos.idcategoria =  $P{c2})  and  year(fechaentrada) <= $P{a} 
and month(fechaentrada) <=  $P{mes}  and inventario.cantidadactual > 0
group by productos.nombre
order by productos.nombre asc]]>
		</queryString>
