			<property name="com.jaspersoft.studio.field.tree.path" value="monedas"/>
		</field>
		<variable name="suma" class="java.lang.Double" calculation="Sum">
			<variableExpression><![CDATA[$F{sum(inventario.cantidadactual)}*$P{factor}]]></variableExpression>
